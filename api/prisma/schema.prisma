generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id         Int    @id
  first_name String @db.VarChar(255)
  last_name  String @db.VarChar(50)

  @@map("_post")
}

model user {
  id         Int    @id
  first_name String @db.VarChar(255)
  last_name  String @db.VarChar(50)
  pass       String @db.VarChar(20)

  @@map("_user")
}
